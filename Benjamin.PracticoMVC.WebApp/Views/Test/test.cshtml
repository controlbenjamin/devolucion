@model Benjamin.PracticoMVC.WebApp.Models.Test.TestModel
@{
    ViewBag.Title = "AgregarUsuario";



}

<h2>Agregar Usuario</h2>
<div class="col-md-6">

    @*chequear parametros*@
    @using (Html.BeginForm())
    {
        <div>

            @*@Html.LabelFor(Model => Model.ObjetoUsuarioCliente.ID_USUARIO)
                @Html.EditorFor(Model => Model.ObjetoUsuarioCliente.ID_USUARIO, new { htmlAttributes = new { @class = "form-control" } })*@



            <label>Selecciones Rol: </label>
            <select id="cboRol" name="cboRol" class="form-control">
                @foreach (var item in Model.ListaDeRoles)
                {
                    <option value="@item.Id">@item.Descripcion</option>

                }

            </select>

            <input id="txtIdRol" name="txtIdRol" type="text" class="form-control" readonly />

            @Html.LabelFor(Model => Model.ObjetoUsuarioCliente.USERNAME)
            @Html.EditorFor(Model => Model.ObjetoUsuarioCliente.USERNAME, new { htmlAttributes = new { @class = "form-control" } })



            @Html.LabelFor(Model => Model.ObjetoUsuarioCliente.NOMBRES)
            @Html.EditorFor(Model => Model.ObjetoUsuarioCliente.NOMBRES, new { htmlAttributes = new { @class = "form-control" } })



            @Html.LabelFor(Model => Model.ObjetoUsuarioCliente.APELLIDOS)
            @Html.EditorFor(Model => Model.ObjetoUsuarioCliente.APELLIDOS, new { htmlAttributes = new { @class = "form-control" } })


            @Html.LabelFor(Model => Model.ObjetoUsuarioCliente.RAZON_SOCIAL)
            @Html.EditorFor(Model => Model.ObjetoUsuarioCliente.RAZON_SOCIAL, new { htmlAttributes = new { @class = "form-control" } })



            @Html.LabelFor(Model => Model.ObjetoUsuarioCliente.ACTIVO)
            @Html.EditorFor(Model => Model.ObjetoUsuarioCliente.ACTIVO)

            <br />
            <br />
            <input type="submit" value="Enviar" class="btn btn-outline-info" />
        </div>
    }

</div>

<script src="~/Scripts/Custom/test.js"></script>